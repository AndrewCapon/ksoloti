// Configure debug launch settings
// Feel free to copy these and modify them for your debugger and MCU
{
    "version": "0.2.0",
    "projectName": "ksoloti",
    "configurations": [
        {
            "name": "STlink launch",
            "cwd": "${workspaceRoot}",
            "executable": "./firmware/build/axoloti.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stlink",
            "device": "STM32F429ZG",
            "interface": "swd",
            "runToEntryPoint": "main",
            "preLaunchTask": "Build all",
            "svdFile": "./debug/STM32F429.svd",
            "swoConfig": {},
            "serverArgs": ["-i 0030001B3532510F31333430", "--frequency 24000"]
        },
        {
            "name": "STlink attach",
            "cwd": "${workspaceRoot}",
            "executable": "./firmware/build/axoloti.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "stlink",
            "device": "STM32F429ZG",
            "interface": "swd",
            "runToEntryPoint": "main",
            "svdFile": "./debug/STM32F429.svd",
            "swoConfig": {},
            "serverArgs": ["-i 0030001B3532510F31333430", "--frequency 24000"]
        },
       ]
}