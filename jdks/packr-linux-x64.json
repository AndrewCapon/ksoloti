{
    "platform": "linux64",
    "jdk": "jdks/zulu21.30.15-ca-jdk21.0.1-linux_x64.tar.gz",
    "executable": "Ksoloti",
    "classpath": [
        "dist/Ksoloti.jar"
    ],
    "removelibs": [
        "dist/Ksoloti.jar"
    ],
    "mainclass": "axoloti.Axoloti",
    "vmargs": [
        "Xms256m",
        "Xmx2g",
        "Xdock:name=Ksoloti",
        "Xbootclasspath/a:lib/marlin-0.9.4.8-Unsafe-OpenJDK11.jar",
        "Dsun.java2d.renderer=org.marlin.pisces.MarlinRenderingEngine",
        "Dsun.java2d.d3d=false"
    ],
    "resources": [
        "chibios",
        "CMSIS",
        "firmware",
        "platform_linux",
        "src/main/java/resources/ksoloti_icon_512.png"
    ],
    "minimizejre": "soft",
    "verbose": true
}
