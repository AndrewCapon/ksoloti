{
    "platform": "windows64",
    "jdk": "jdks/zulu21.30.15-ca-jdk21.0.1-win_x64.zip",
    "executable": "Ksoloti",
    "classpath": [
        "dist/Ksoloti.jar"
    ],
    "removelibs": [
        "dist/Ksoloti.jar"
    ],
    "mainclass": "axoloti.Axoloti",
    "vmargs": [
        "Xms256m",
        "Xmx2g",
        "Xbootclasspath/a:lib/marlin-0.9.4.8-Unsafe-OpenJDK11.jar",
        "Dsun.java2d.renderer=org.marlin.pisces.MarlinRenderingEngine",
        "Dsun.java2d.dpiaware=true"
    ],
    "resources": [
        "chibios",
        "CMSIS",
        "firmware",
        "firmware_axoloti_legacy",
        "platform_win"
    ],
    "minimizejre": "soft",
    "output": "packagetemp/win/ksoloti-1.0.12",
    "verbose": true
}
